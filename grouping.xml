<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="grouping">


	<groups>
		<run>
			<include name="dataDriven"></include>
			<exclude name="functional"></exclude>
		</run>
	</groups>
	<listeners>
		<listener class-name="utilities.ExtentReportUtility"></listener>
	</listeners>
	<test thread-count="5" name="Test">
		<parameter name="browser" value="Edge"></parameter>
		<parameter name="os" value="windows"></parameter>
		<parameter name="filePath"
			value="./testData/TestData_AccRegistraton_booking.com.xlsx"></parameter>
		<classes>
			<class name="testcases.TC_001_001_RegistrationUsingEmail" />
			<class name="testcases.TC_001_001_RegistrationUsingEmailDDT" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
